<?xml version="1.0" encoding="UTF-8"?>
<ControlFile>
	<!--
	OutputLevel can be:
	0=None
	1=Just major events,
	2=All SS activities,
	3=Maximum output
 	-->
	<OutputLevel>1</OutputLevel>
	<Title>Cellular PDMs and DSRC BSMs example</Title>
	<Seed>342554</Seed>
	<InputFiles>
		<TrajectoryFile>
			<FileType>csv</FileType>
			<FileName>sample_csv.csv</FileName>
			<CSVTrajectoryFileFields>
				<X>x-ft</X>
				<Y>y-ft</Y>
				<Time>secs</Time>
				<ID>veh</ID>
				<Speed>speed (mph)</Speed>	
			</CSVTrajectoryFileFields>		
		</TrajectoryFile>
		<StrategyFile>Strategy_Sample_Cellular.xml</StrategyFile>		
	</InputFiles>
	<EquippedVehicles>
		<PDMMarketPenetration>50</PDMMarketPenetration>	
		<BSMMarketPenetration>50</BSMMarketPenetration>			
	</EquippedVehicles>	
	<OutputFiles>
		<PDMAllFile>PDMTCAoutALL.csv</PDMAllFile>
		<PDMTransFile>PDMTCAoutTrans.csv</PDMTransFile>
		<BSMTransFile>BSMTCAoutTrans.csv</BSMTransFile>		
		<BSMExtendedFile>BSMExtendedTrans.csv</BSMExtendedFile>		
	</OutputFiles>
	<BSMExtendedFlag>1</BSMExtendedFlag>
</ControlFile>