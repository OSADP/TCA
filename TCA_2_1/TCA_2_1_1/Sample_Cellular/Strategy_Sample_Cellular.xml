<?xml version="1.0" encoding="UTF-8" ?> 
<!-- This sample strategy file enables PDM vehicles
  to transmit via cellular. Three cellular regions
  are defined with different latency and loss rates.-->
<Strategy>
  <Title>Sample Cellular Example</Title>
  <Inputs> 
    <Cellular>
    <!-- If cellular communication is used: 
      0: Not used
      1: Cellular comm used (Vehicles transmit snapshots via cellular according to defined latency) -->
      <PDMCellularFlag>1</PDMCellularFlag>
      <BSMCellularFlag>1</BSMCellularFlag>
      <MinNumberofSStoTransmitViaCellular>1</MinNumberofSStoTransmitViaCellular>
      <DefaultLossPercent>3</DefaultLossPercent>
      <DefaultLatency>3000</DefaultLatency>
      <Regions>
      <Region1>
        <Name>Cellular_Region1</Name>
        <Point1>-22585000,15500000</Point1>
        <Point2>-22555000,15480000</Point2>
        <LossPercent>2</LossPercent>
        <Latency>2000</Latency>
      </Region1>
      <Region2>
        <Name>Cellular_Region2</Name>
        <Point1>-22554999,15490000</Point1>
        <Point2>-22510000,15470000</Point2>
        <LossPercent>1</LossPercent>
        <Latency>1000</Latency>
      </Region2>
      <Region3>
        <Name>Cellular_Region3</Name>
        <Point1>-22540000,15450000</Point1>
        <Point2>-22495000,15440000</Point2>
        <LossPercent>92</LossPercent>
        <Latency>5000</Latency>
      </Region3>
    </Regions>
    </Cellular>
  </Inputs>
</Strategy>